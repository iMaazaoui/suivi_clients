<!DOCTYPE html>
<html>
<head>
  <title>SuiviClients</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
	<div class="container">
		<% flash.each do |key, value| %>
			<div class="alert alert-danger"><%= value %> en tantque <%= session[:nom_prenom] %></div>
		<% end %>
		<%= render 'layouts/header' %>
		<div class="row">
			<%= yield %>
		</div>
		<%= render 'layouts/footer' %>
	</div>
</body>
</html>
